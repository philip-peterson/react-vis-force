{
  "name": "react-vis-force",
  "version": "0.1.4",
  "main": "./dist/react-vis-force.js",
  "homepage": "http://uber.github.io/react-vis-force",
  "licence": "MIT",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "autoprefixer": "6.4.1",
    "babel-core": "6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "^2.11.14",
    "cross-spawn": "4.0.0",
    "css-loader": "0.24.0",
    "d3-scale": "^0.7.0",
    "detect-port": "1.0.0",
    "dotenv": "2.0.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-react-app": "^0.2.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.22.0",
    "http-proxy-middleware": "0.17.1",
    "jest": "^16.0.1",
    "json-loader": "0.5.4",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "react": "^15.3.1",
    "react-addons-perf": "^15.3.2",
    "react-addons-test-utils": "^15.3.1",
    "react-dev-utils": "^0.2.1",
    "react-dom": "^15.3.1",
    "react-prism": "^4.0.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "sinon": "^1.17.6",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "uber-licence": "^2.0.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.1",
    "whatwg-fetch": "1.0.0"
  },
  "peerDependencies": {
    "react": "0.14.x - 15.x",
    "react-dom": "0.14.x - 15.x"
  },
  "dependencies": {
    "d3-force": "^1.0.2",
    "global": "^4.3.0",
    "prop-types": "^15.6.1",
    "react-addons-shallow-compare": "0.14.x - 15.x"
  },
  "scripts": {
    "start": "npm run storybook",
    "build": "node scripts/build.js",
    "build:docs": "npm run build-storybook",
    "test": "npm run lint && npm run unittest",
    "test:ci": "npm run lint && npm run coverage && cat ./coverage/lcov.info | coveralls",
    "unittest": "node scripts/test.js --env=jsdom",
    "lint": "eslint .",
    "coverage": "CI=true npm run unittest -- --coverage",
    "check-licence": "uber-licence --dry",
    "add-licence": "uber-licence",
    "deploy-docs": "./bin/deploy-docs.sh",
    "prepublish": "npm run coverage && npm run build && npm run add-licence",
    "postpublish": "npm run build:docs && npm run deploy-docs",
    "ci:postsuccess": "npm run build:docs && npm run deploy-docs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o build"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
